{
  "name": "rollup-plugin-netlify-optimize",
  "version": "0.0.1",
  "description": "Optimizes a single-page-application for hosting on netlify. Pre-renders pages with skeleton html",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "repository": "https://github.com/calebeby/rollup-plugin-netlify-optimize",
  "license": "MIT",
  "keywords": [
    "rollup-plugin"
  ],
  "files": [
    "build"
  ],
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.51",
    "@types/hasha": "3.0.0",
    "@types/jest": "23.1.0",
    "jest": "23.1.0",
    "preact": "8.2.9",
    "preact-render-to-string": "3.7.0",
    "rollup": "0.60.7",
    "ts-jest": "22.4.6",
    "typescript": "2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "hasha": "^3.0.0",
    "rollup-plugin-babel": "^4.0.0-beta.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-terser": "^1.0.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
